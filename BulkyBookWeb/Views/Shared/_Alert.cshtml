@if (TempData["success"] != null)
{
    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <style href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet"></style>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script type="text/javascript">
     toastr.success('@TempData["success"]')   
    </script>
    
}

@if (TempData["error"] != null)
{
    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://raw.githubusercontent.com/rosshatokay/toasterjs/main/src/toaster.js"></script>
    <h2>@TempData["error"]</h2>
}

